pipeline {
    agent any 
    tools {
        jdk 'jdk11'
        maven 'maven3'
    }
    stages{
        stage("SonarQube Analysis"){
            steps{
                script{
                    withSonarQubeEnv(credentialsId: 'sonar-token') {
                        sh 'mvn sonar:sonar'
                    }
                }
            }
        }
    }
}